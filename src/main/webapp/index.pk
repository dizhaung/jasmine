<article>
    <div class="container">
        <div class="col-md-8">
            <div class="row-fluid" ng-repeat="model in models">
                <div class="thumbnail">
                    <div class="caption">
                        <h3 ng-bind="model.blogName"></h3>
                        <div class="caption">
                            <span class="glyphicon glyphicon-calendar" aria-hidden="true"></span>
                            {{model.blogCreateTime + "前"}}
                            &nbsp; | &nbsp;
                            <span class="glyphicon glyphicon-flag" aria-hidden="true"></span>
                            {{model.menuName}}
                            &nbsp; | &nbsp;
                            <span class="glyphicon glyphicon-user" aria-hidden="true"></span>
                            <a href="" class="" data-toggle="tooltip" data-placement="top" title="由admin发布">admin</a>
                            &nbsp; | &nbsp;
                            <span class="glyphicon glyphicon-comment" aria-hidden="true"></span>
                            7条评论
                            &nbsp; | &nbsp;
                            <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>
                            {{model.views + " views"}}
                        </div>
                        <div class="caption" style="overflow:hidden;white-space:nowrap;text-overflow:ellipsis;" ng-bind="model.blogContent"></div>
                        <p>
                            <a class="btn btn-primary" href="#">浏览</a>
                            <a class="btn" href="#">分享</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <!-- 查询 -->
            <div class="thumbnail">
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
